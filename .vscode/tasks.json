{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run IG Publisher build",
            "type": "shell",
            "command": "/bin/bash _genonce.sh",
            "group": {
              "kind": "build",
              "isDefault": true
            }
        },
        {
            "label": "Trigger auto build - master branch",
            "type": "shell",
            "command": "curl -X POST \"https://us-central1-fhir-org-starter-project.cloudfunctions.net/ig-commit-trigger\" -H \"Content-type: application/json\" --data '{\"ref\": \"refs/heads/master\", \"repository\": {\"full_name\": \"AuDigitalHealth/ci-fhir-r4\"}}'",
            "group": {
              "kind": "build"
            }
        },
        {
          "label": "Download latest IG Publisher",
          "type": "shell",
          "command": "/bin/bash _updatePublisher.sh",
          "group": {
            "kind": "build"
          }
        }
    ]
}


