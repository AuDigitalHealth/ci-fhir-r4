<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="dh-practitioner-core-1"/>
  <url value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-practitioner-core-1"/>
  <version value="0.0.1"/>
  <name value="DHPractitionerCore"/>
  <title value="ADHA Core Practitioner"/>
  <status value="draft"/>
  <experimental value="false"/>
  <publisher value="Australian Digital Health Agency"/>
  <contact>
    <telecom>
      <system value="email"/>
      <value value="help@digitalhealth.gov.au"/>
    </telecom>
  </contact>
  <description
    value="The purpose of this profile is to define a core representation a practitioner for the electronic exchange of health information between individuals, healthcare providers, and the My Health Record system infrastructure in Australia."/>
  <copyright
    value="Copyright Â© 2022 Australian Digital Health Agency - All rights reserved. This content is licensed under a Creative Commons Attribution 4.0 International License. See https://creativecommons.org/licenses/by/4.0/."/>
  <fhirVersion value="4.0.1"/>
  <mapping>
    <identity value="CDA.assignedPerson"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="Agency CDA Schema assignedPerson"/>
  </mapping>
  <mapping>
    <identity value="CDA.informant"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="Agency CDA Schema informant"/>
  </mapping>
  <mapping>
    <identity value="CDA.informationRecipient"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="Agency CDA Schema informationRecipient"/>
  </mapping>
  <mapping>
    <identity value="CDA.author"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="Agency CDA Schema author"/>
  </mapping>
  <kind value="resource"/>
  <abstract value="false"/>
  <type value="Practitioner"/>
  <baseDefinition value="http://hl7.org.au/fhir/StructureDefinition/au-practitioner"/>
  <derivation value="constraint"/>
  <differential>
    <element id="Practitioner">
      <path value="Practitioner"/>
      <short value="A person with a formal responsibility in the provisioning of services"/>
      <constraint>
        <key value="inv-dh-prac-01"/>
        <severity value="error"/>
        <human value="The practitioner shall at least have an identifier or a name"/>
        <expression value="identifier.exists() or name.exists()"/>
        <source
          value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-practitioner-core-1"
        />
      </constraint>
      <constraint>
        <key value="inv-dh-prac-03"/>
        <severity value="error"/>
        <human value="One practitioner name shall have at least a family name"/>
        <expression value="name.family.exists()"/>
        <source
          value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-practitioner-core-1"
        />
      </constraint>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".author"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map value="ClinicalDocument.informationRecipient.intendedRecipient.informationRecipient"/>
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value=".assignedPerson"/>
      </mapping>
    </element>
    <element id="Practitioner.identifier">
      <path value="Practitioner.identifier"/>
      <definition value="An identifier that applies to this person in this role."/>
      <slicing>
        <discriminator>
          <type value="pattern"/>
          <path value="type"/>
        </discriminator>
        <rules value="open"/>
      </slicing>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".author.assignedAuthor.assignedPerson.ext:asEntityIdentifier"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant.assignedEntity.assignedPerson.ext:asEntityIdentifier"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map value="ClinicalDocument.informationRecipient.ext:asEntityIdentifier"/>
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value=".assignedPerson.ext:asEntityIdentifier"/>
      </mapping>
    </element>
    <element id="Practitioner.identifier:hpii">
      <path value="Practitioner.identifier"/>
      <sliceName value="hpii"/>
      <max value="1"/>
      <type>
        <code value="Identifier"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/au-hpii"/>
      </type>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".author.assignedAuthor.assignedPerson.ext:asEntityIdentifier"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant.assignedEntity.assignedPerson.ext:asEntityIdentifier"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.informationRecipient.intendedRecipient.informationRecipient.ext:asEntityIdentifier"
        />
      </mapping>
    </element>
    <element id="Practitioner.active">
      <path value="Practitioner.active"/>
      <fixedBoolean value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value="n/a"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value="n/a"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map value="n/a"/>
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value="n/a"/>
      </mapping>
    </element>
    <element id="Practitioner.name">
      <path value="Practitioner.name"/>
      <min value="1"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".author.assignedAuthor.assignedPerson.name"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant.assignedEntity.assignedPerson.name"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.informationRecipient.intendedRecipient.informationRecipient.name"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value=".assignedPerson.name"/>
      </mapping>
    </element>
    <element id="Practitioner.telecom">
      <path value="Practitioner.telecom"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".author.assignedAuthor.telecom"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant.assignedEntity.telecom"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map value="ClinicalDocument.informationRecipient.intendedRecipient.telecom"/>
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value=".telecom"/>
      </mapping>
    </element>
    <element id="Practitioner.address">
      <path value="Practitioner.address"/>
      <definition
        value="Address(es) of the practitioner that are not role specific (typically home address). Work addresses are not typically entered in this property as they are usually role dependent."/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".addr"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant.assignedEntity.addr"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map value="ClinicalDocument.informationRecipient.intendedRecipient.addr"/>
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value=".addr"/>
      </mapping>
    </element>
    <element id="Practitioner.qualification">
      <path value="Practitioner.qualification"/>
      <constraint>
        <key value="inv-dh-prac-02"/>
        <severity value="error"/>
        <human
          value="If present, an issuer shall at least have a reference that conforms to Identified Organization, an identifier or a display"/>
        <expression
          value="issuer.exists() implies issuer.all(($this.reference.resolve().conformsTo('http://ns.electronichealth.net.au/fhir/StructureDefinition/organization-identified-1')) or $this.identifier.exists() or $this.display.exists())"/>
        <source
          value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-practitioner-core-1"
        />
      </constraint>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value="Instantiated as assignedPerson.ext:asQualifications, or ClinicalDocument.component.structuredBody.component.section.ext:coverage2"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map
          value="Instantiated as assignedPerson.ext:asQualifications, or ClinicalDocument.component.structuredBody.component.section.ext:coverage2"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="Instantiated as ClinicalDocument.informationRecipient.intendedRecipient.informationRecipient.ext:asQualifications, or ClinicalDocument.component.structuredBody.component.section.ext:coverage2"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map
          value="Instantiated as assignedPerson.ext:asQualifications, or ClinicalDocument.component.structuredBody.component.section.ext:coverage2"
        />
      </mapping>
    </element>
    <element id="Practitioner.qualification.identifier">
      <path value="Practitioner.qualification.identifier"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:id"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:id"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:id"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:id"
        />
      </mapping>
    </element>
    <element id="Practitioner.qualification.code">
      <path value="Practitioner.qualification.code"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:code"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:code"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:code"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:code"
        />
      </mapping>
    </element>
    <element id="Practitioner.qualification.period">
      <path value="Practitioner.qualification.period"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:effectiveTime"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:effectiveTime"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:effectiveTime"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:effectiveTime"
        />
      </mapping>
    </element>
    <element id="Practitioner.qualification.issuer">
      <path value="Practitioner.qualification.issuer"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:participant"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:participant"
        />
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:participant"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map
          value="ClinicalDocument.component.structuredBody.component.section.ext:coverage2.ext:entitlement.ext:participant"
        />
      </mapping>
    </element>
    <element id="Practitioner.communication">
      <path value="Practitioner.communication"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value=".assignedPerson.ext:languageCommunication"/>
      </mapping>
      <mapping>
        <identity value="CDA.informant"/>
        <map value=".informant.assignedEntity.assignedPerson.ext:languageCommunication"/>
      </mapping>
      <mapping>
        <identity value="CDA.informationRecipient"/>
        <map
          value="ClinicalDocument.informationRecipient.intendedRecipient.informationRecipient.ext:languageCommunication"
        />
      </mapping>
      <mapping>
        <identity value="CDA.assignedPerson"/>
        <map value=".assignedPerson.ext:languageCommunication"/>
      </mapping>
    </element>
  </differential>
</StructureDefinition>
