<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="dh-relatedperson-core-1"/>
  <url value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-relatedperson-core-1"/>
  <version value="0.0.1"/>
  <name value="DHRelatedPersonCore"/>
  <title value="ADHA Core RelatedPerson"/>
  <status value="draft"/>
  <experimental value="false"/>
  <publisher value="Australian Digital Health Agency"/>
  <contact>
    <telecom>
      <system value="email"/>
      <value value="help@digitalhealth.gov.au"/>
    </telecom>
  </contact>
  <description
    value="The purpose of this profile is to provide a core representation of a related person for the electronic exchange of health information between individuals, healthcare providers, and the My Health Record system infrastructure in Australia."/>
  <copyright
    value="Copyright Â© 2022 Australian Digital Health Agency - All rights reserved. This content is licensed under a Creative Commons Attribution 4.0 International License. See https://creativecommons.org/licenses/by/4.0/."/>
  <fhirVersion value="4.0.1"/>
  <mapping>
    <identity value="CDA.author"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="Agency CDA Schema author"/>
  </mapping>
  <kind value="resource"/>
  <abstract value="false"/>
  <type value="RelatedPerson"/>
  <baseDefinition value="http://hl7.org.au/fhir/StructureDefinition/au-relatedperson"/>
  <derivation value="constraint"/>
  <differential>
    <element id="RelatedPerson">
      <path value="RelatedPerson"/>
      <constraint>
        <key value="inv-dh-relper-01"/>
        <severity value="error"/>
        <human
          value="The related person shall at least have an identifier or a name or a relationship with the patient"/>
        <expression value="identifier.exists() or name.exists() or relationship.exists()"/>
        <source
          value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-relatedperson-core-1"
        />
      </constraint>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.identifier">
      <path value="RelatedPerson.identifier"/>
      <definition value="Identifier for a person within a particular scope."/>
      <slicing>
        <discriminator>
          <type value="pattern"/>
          <path value="type"/>
        </discriminator>
        <rules value="open"/>
      </slicing>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson.ext:asEntityIdentifier"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.identifier:ihi">
      <path value="RelatedPerson.identifier"/>
      <sliceName value="ihi"/>
      <max value="1"/>
      <type>
        <code value="Identifier"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/au-ihi"/>
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="RelatedPerson.identifier:medicare">
      <path value="RelatedPerson.identifier"/>
      <sliceName value="medicare"/>
      <max value="1"/>
      <type>
        <code value="Identifier"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/au-medicarecardnumber"/>
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="RelatedPerson.identifier:dva">
      <path value="RelatedPerson.identifier"/>
      <sliceName value="dva"/>
      <max value="1"/>
      <type>
        <code value="Identifier"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/au-dvanumber"/>
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="RelatedPerson.active">
      <path value="RelatedPerson.active"/>
      <fixedBoolean value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map value="n/a"/>
      </mapping>
    </element>
    <element id="RelatedPerson.patient">
      <path value="RelatedPerson.patient"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-patient-core-1"/>
      </type>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value="This data is considered to be included via induction in ClinicalDocument.recordTarget.patientRole."
        />
      </mapping>
    </element>
    <element id="RelatedPerson.relationship">
      <path value="RelatedPerson.relationship"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson.ext:personalRelationship.ext:code"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.name">
      <path value="RelatedPerson.name"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson.name"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.telecom">
      <path value="RelatedPerson.telecom"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].telecom"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.address">
      <path value="RelatedPerson.address"/>
      <definition value="Address where the related person can be contacted or visited."/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].addr"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.communication">
      <path value="RelatedPerson.communication"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson.ext:languageCommunication"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.communication.language">
      <path value="RelatedPerson.communication.language"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson.ext:languageCommunication.languageCode"
        />
      </mapping>
    </element>
    <element id="RelatedPerson.communication.preferred">
      <path value="RelatedPerson.communication.preferred"/>
      <mustSupport value="true"/>
      <mapping>
        <identity value="CDA.author"/>
        <map
          value=".author.assignedAuthor[code[code='AGNT, codeSystem='2.16.840.1.113883.5.110']].assignedPerson.ext:languageCommunication.preferenceInd"
        />
      </mapping>
    </element>
  </differential>
</StructureDefinition>
