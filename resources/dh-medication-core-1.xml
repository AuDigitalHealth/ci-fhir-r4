<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="dh-medication-core-1"/>
  <url value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-medication-core-1"/>
  <version value="0.0.1"/>
  <name value="DHMedicationCore"/>
  <title value="ADHA Core Medication"/>
  <status value="draft" />
  <experimental value="true" />
  <publisher value="Australian Digital Health Agency" />
  <contact>
    <telecom>
      <system value="email" />
      <value value="help@digitalhealth.gov.au" />
    </telecom>
  </contact>
  <description value="The purpose of this profile is to provide a core representation of a medication for the electronic exchange of health information between individuals, healthcare providers, and the My Health Record system infrastructure in Australia." />
  <copyright value="Copyright Â© 2022 Australian Digital Health Agency - All rights reserved. This content is licensed under a Creative Commons Attribution 4.0 International License. See https://creativecommons.org/licenses/by/4.0/." />
  <fhirVersion value="4.0.1" />
  <mapping>
    <identity value="CDA.manufacturedProduct"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="Agency CDA Schema manufacturedProduct"/>
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Medication" />
  <baseDefinition value="http://hl7.org.au/fhir/StructureDefinition/au-medication" />
  <derivation value="constraint" />
  <differential>
    <element id="Medication">
      <path value="Medication" />
      <constraint>
        <key value="inv-dh-med-01" />
        <severity value="error" />
        <human value="If present, a manufacturer shall at least have a reference, an identifier or a display" />
        <expression value="manufacturer.exists() implies manufacturer.all($this.reference.exists() or $this.identifier.exists() or $this.display.exists())" />
        <source value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-medication-core-1" />
      </constraint>
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct"/>
      </mapping>
    </element>
    <element id="Medication.extension">
      <path value="Medication.extension"/>
      <slicing>
        <discriminator>
          <type value="value"/>
          <path value="url"/>
        </discriminator>
        <rules value="open"/>
      </slicing>
      <mapping>
        <identity value="CDA.act"/>
        <map value="n/a"/>
      </mapping>
    </element>
    <element id="Medication.extension:brandName">
      <path value="Medication.extension" />
      <sliceName value="brandName" />
      <short value="Medication brand name" />
      <mustSupport value="true" />
      <type>
        <code value="Extension"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/medication-brand-name"/>
      </type>
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".entryRelationship.brand.act.text"/>
      </mapping>
    </element>
    <element id="Medication.extension:genericName">
      <path value="Medication.extension" />
      <sliceName value="genericName" />
      <short value="Medication generic drug name" />
      <type>
        <code value="Extension"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/medication-generic-name"/>
      </type>
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".entryRelationship.brand.act.text"/>
      </mapping>
    </element>
    <element id="Medication.code">
      <path value="Medication.code" />
      <min value="1" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.code"/>
      </mapping>
    </element>
    <element id="Medication.manufacturer">
      <path value="Medication.manufacturer" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-organization-core-1" />
      </type>
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturerOrganization"/>
      </mapping>
    </element>
    <element id="Medication.form">
      <path value="Medication.form" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.ext:formCode"/>
      </mapping>
    </element>
    <element id="Medication.amount">
      <path value="Medication.amount" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.ext:asIngredient.ext:quantity"/>
      </mapping>
    </element>
    <element id="Medication.ingredient">
      <path value="Medication.ingredient" />
      <constraint>
        <key value="inv-dh-med-02" />
        <severity value="error" />
        <human value="If present, an item reference shall at least have a reference or a display" />
        <expression value="itemReference.exists() implies itemReference.all($this.reference.exists() or $this.display.exists())" />
        <source value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-medication-core-1" />
      </constraint>
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.ext:asIngredient.ext:ingredientManufacturedMaterial"/>
      </mapping>
    </element>
    <element id="Medication.ingredient.item[x]">
      <path value="Medication.ingredient.item[x]" />
      <mustSupport value="true" />
      <type>
        <code value="CodeableConcept" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-substance-core-1" />
        <targetProfile value="http://ns.electronichealth.net.au/fhir/StructureDefinition/dh-medication-core-1" />
      </type>
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.ext:asIngredient.ext:ingredientManufacturedMaterial.ext:quantity"/>
      </mapping>
    </element>
    <element id="Medication.ingredient.isActive">
      <path value="Medication.ingredient.isActive" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value="n/a"/>
      </mapping>
    </element>
    <element id="Medication.ingredient.strength">
      <path value="Medication.ingredient.strength" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value="TBD"/>
      </mapping>
    </element>
    <element id="Medication.batch">
      <path value="Medication.batch" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value="n/a"/>
      </mapping>
    </element>
    <element id="Medication.batch.lotNumber">
      <path value="Medication.batch.lotNumber" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.lotNumberText"/>
      </mapping>
    </element>
    <element id="Medication.batch.expirationDate">
      <path value="Medication.batch.expirationDate" />
      <mustSupport value="true" />
      <mapping>
        <identity value="CDA.manufacturedProduct"/>
        <map value=".manufacturedProduct.manufacturedMaterial.ext:expirationTime"/>
      </mapping>
    </element>
  </differential>
</StructureDefinition>