  <kind value="resource"/>
  <abstract value="false"/>
  <type value="Composition"/>
  <baseDefinition value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-composition-document-1"/>
  <derivation value="constraint"/>
  <differential>
    <element id="Composition">
      <path value="Composition"/>
      <short value="Personal Health Notes"/>
    </element>
    <element id="Composition.meta">
      <path value="Composition.meta"/>
      <min value="1"/>
    </element>
    <element id="Composition.meta.profile">
      <path value="Composition.meta.profile" />
      <min value="1"/>
      <patternCanonical value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-composition-phn-1" />
    </element>
    <element id="Composition.status">
      <path value="Composition.status"/>
      <patternCode value="final"/>
    </element>
    <element id="Composition.type">
      <path value="Composition.type"/>
      <short value="Sub type of advance care planning document"/>
      <patternCodeableConcept>
        <coding>
          <system value="https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1" />
          <code value="100.16681" />
        </coding>
      </patternCodeableConcept>
    </element>
    <element id="Composition.category">
      <path value="Composition.category"/>
      <patternCodeableConcept>
        <coding>
          <system value="https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1" />
          <code value="100.16681" />
        </coding>
      </patternCodeableConcept>
    </element>
    <element id="Composition.author">
      <path value="Composition.author" />
      <short value="Author of the composition, may not be the author of the attached advance care planning document" />
      <max value="1" />
