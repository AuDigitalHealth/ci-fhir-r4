        <coding>
          <system value="https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1" />
          <code value="102.15513" />
        </coding>
      </patternCodeableConcept>
    </element>
    <element id="Composition.section.text">
      <path value="Composition.section.text"/>
      <min value="1"/>
