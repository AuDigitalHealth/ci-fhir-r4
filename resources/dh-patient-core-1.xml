<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="dh-patient-core-1"/>
  <url value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-patient-core-1"/>
  <version value="0.0.1"/>
  <name value="DHPatientCore"/>
  <title value="ADHA Core Patient"/>
  <status value="draft"/>
  <experimental value="false"/>
  <publisher value="Australian Digital Health Agency"/>
  <contact>
    <telecom>
      <system value="email"/>
      <value value="help@digitalhealth.gov.au"/>
    </telecom>
  </contact>
  <description
    value="The purpose of this profile is to provide a core representation of a patient for the electronic exchange of health information between individuals, healthcare providers, and the My Health Record system infrastructure in Australia."/>
  <copyright
    value="Copyright Â© 2022 Australian Digital Health Agency - All rights reserved. This content is licensed under a Creative Commons Attribution 4.0 International License. See https://creativecommons.org/licenses/by/4.0/."/>
  <fhirVersion value="4.0.1"/>
  <mapping>
     <identity value="CDA"/>
    <uri value="http://hl7.org/v3/cda"/>
    <name value="CDA (R2)"/>
  </mapping>
  <kind value="resource"/>
  <abstract value="false"/>
  <type value="Patient"/>
  <baseDefinition value="http://hl7.org.au/fhir/StructureDefinition/au-patient"/>
  <derivation value="constraint"/>
  <differential>
    <element id="Patient">
      <path value="Patient"/>
      <short value="A patient in the context of electronic exchange of health information"/>
      <constraint>
        <key value="inv-dh-pat-01"/>
        <severity value="error"/>
        <human
          value="If present, a general practitioner shall at least have a reference, an identifier or a display"/>
        <expression
          value="generalPractitioner.exists() implies generalPractitioner.all($this.reference.exists() or $this.identifier.exists() or $this.display.exists())"/>
        <source
          value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-patient-core-1"/>
      </constraint>
      <constraint>
        <key value="inv-dh-pat-02"/>
        <severity value="error"/>
        <human value="At least one patient identifier shall at least have a system and a value"/>
        <expression value="identifier.where(system.count() + value.count() &gt;1).exists()"/>
        <source
          value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-patient-core-1"/>
      </constraint>
      <constraint>
        <key value="inv-dh-pat-03"/>
        <severity value="error"/>
        <human value="If present, date of arrival shall be year of arrival"/>
        <expression
          value="extension('http://hl7.org.au/fhir/StructureDefinition/date-of-arrival').exists() implies extension.valueDate.value.matches('^([0-9]{4})$')"/>
        <source
          value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-patient-core-1"/>
      </constraint>
      <mapping>
         <identity value="CDA"/>
        <map value="/ClinicalDocument/recordTarget/templateId/@root='TBD'"/>
      </mapping>
      <mapping>
        <identity value="CDA"/>
        <map value="/ClinicalDocument/recordTarget/patientRole/"/>
      </mapping>
    </element>
    <element id="Patient.extension:birthPlace">
      <path value="Patient.extension"/>
      <sliceName value="birthPlace"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/birthplace"/>
      </mapping>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/birthplace/name"/>
      </mapping>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/birthplace/addr"/>
      </mapping>
    </element>
    <element id="Patient.extension:indigenousStatus">
      <path value="Patient.extension"/>
      <sliceName value="indigenousStatus"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/ethnicGroupCode"/>
        <comment
          value="Binding: https://healthterminologies.gov.au/fhir/ValueSet/australian-indigenous-status-1 (required)"
        />
      </mapping>
    </element>
    <element id="Patient.extension:interpreterRequired">
      <path value="Patient.extension"/>
      <sliceName value="interpreterRequired"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/languageCommunication/ext:interpreterRequiredInd"/>
      </mapping>
    </element>
    <element id="Patient.extension:dateOfArrival">
      <path value="Patient.extension"/>
      <sliceName value="dateOfArrival"/>
      <short value="Year of Arrival"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="TBD"/>
      </mapping>
    </element>
    <element id="Patient.identifier">
      <path value="Patient.identifier"/>
      <min value="1"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/ext:asEntityIdentifier"/>
      </mapping>
    </element>
    <element id="Patient.active">
      <path value="Patient.active"/>
      <fixedBoolean value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="n/a"/>
      </mapping>
    </element>
    <element id="Patient.name">
      <path value="Patient.name"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/name"/>
      </mapping>
    </element>
    <element id="Patient.telecom">
      <path value="Patient.telecom"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="telecom"/>
      </mapping>
    </element>
    <element id="Patient.gender">
      <path value="Patient.gender"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/administrativeGenderCode"/>
      </mapping>
    </element>
    <element id="Patient.birthDate">
      <path value="Patient.birthDate"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/birthTime"/>
      </mapping>
    </element>
    <element id="Patient.birthDate.extension:accuracyIndicator">
      <path value="Patient.birthDate.extension"/>
      <sliceName value="accuracyIndicator"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="TBD"/>
      </mapping>
    </element>
    <element id="Patient.birthDate.extension:birthTime">
      <path value="Patient.birthDate.extension"/>
      <sliceName value="birthTime"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/birthTime"/>
      </mapping>
    </element>
    <element id="Patient.address">
      <path value="Patient.address"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="addr"/>
      </mapping>
    </element>
    <element id="Patient.communication">
      <path value="Patient.communication"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="/patient/languageCommunication"/>
      </mapping>
    </element>
    <element id="Patient.communication.language">
      <path value="Patient.communication.language"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/languageCommunication/languageCode"/>
      </mapping>
    </element>
    <element id="Patient.communication.preferred">
      <path value="Patient.communication.preferred"/>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="patient/languageCommunication/preferenceInd"/>
      </mapping>
    </element>
    <element id="Patient.generalPractitioner">
      <path value="Patient.generalPractitioner"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-organization-core-1"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/dh-practitionerrole-core-1"
        />
      </type>
      <mustSupport value="true"/>
      <mapping>
         <identity value="CDA"/>
        <map value="/ClinicalDocument/participant"/>
      </mapping>
    </element>
  </differential>
</StructureDefinition>
